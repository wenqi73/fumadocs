{
  "name": "fumadocs-core",
  "version": "13.4.10",
  "description": "The library for building a documentation website in Next.js",
  "keywords": [
    "NextJs",
    "Docs"
  ],
  "homepage": "https://fumadocs.vercel.app",
  "repository": "github:fuma-nama/fumadocs",
  "license": "MIT",
  "author": "Fuma Nama",
  "type": "module",
  "exports": {
    "./sidebar": {
      "import": "./dist/sidebar.js",
      "types": "./dist/sidebar.d.ts"
    },
    "./breadcrumb": {
      "import": "./dist/breadcrumb.js",
      "types": "./dist/breadcrumb.d.ts"
    },
    "./toc": {
      "import": "./dist/toc.js",
      "types": "./dist/toc.d.ts"
    },
    "./search/client": {
      "import": "./dist/search/client.js",
      "types": "./dist/search/client.d.ts"
    },
    "./search/shared": {
      "import": "./dist/search/shared.js",
      "types": "./dist/search/shared.d.ts"
    },
    "./search/server": {
      "import": "./dist/search/server.js",
      "types": "./dist/search/server.d.ts"
    },
    "./server": {
      "import": "./dist/server/index.js",
      "types": "./dist/server/index.d.ts"
    },
    "./source": {
      "import": "./dist/source/index.js",
      "types": "./dist/source/index.d.ts"
    },
    "./utils/use-on-change": {
      "import": "./dist/utils/use-on-change.js",
      "types": "./dist/utils/use-on-change.d.ts"
    },
    "./link": {
      "import": "./dist/link.js",
      "types": "./dist/link.d.ts"
    },
    "./middleware": {
      "import": "./dist/middleware.js",
      "types": "./dist/middleware.d.ts"
    },
    "./mdx-plugins": {
      "import": "./dist/mdx-plugins/index.js",
      "types": "./dist/mdx-plugins/index.d.ts"
    },
    "./search-algolia/client": {
      "import": "./dist/search-algolia/client.js",
      "types": "./dist/search-algolia/client.d.ts"
    },
    "./search-algolia/server": {
      "import": "./dist/search-algolia/server.js",
      "types": "./dist/search-algolia/server.d.ts"
    },
    "./i18n": {
      "import": "./dist/i18n/index.js",
      "types": "./dist/i18n/index.d.ts"
    }
  },
  "typesVersions": {
    "*": {
      "sidebar": [
        "./dist/sidebar.d.ts"
      ],
      "breadcrumb": [
        "./dist/breadcrumb.d.ts"
      ],
      "toc": [
        "./dist/toc.d.ts"
      ],
      "search/client": [
        "./dist/search/client.d.ts"
      ],
      "search/shared": [
        "./dist/search/shared.d.ts"
      ],
      "search/server": [
        "./dist/search/server.d.ts"
      ],
      "server": [
        "./dist/server/index.d.ts"
      ],
      "source": [
        "./dist/source/index.d.ts"
      ],
      "link": [
        "./dist/link.d.ts"
      ],
      "middleware": [
        "./dist/middleware.d.ts"
      ],
      "utils/use-on-change": [
        "./dist/utils/use-on-change.d.ts"
      ],
      "mdx-plugins": [
        "./dist/mdx-plugins/index.d.ts"
      ],
      "search-algolia/client": [
        "./dist/search-algolia/client.d.ts"
      ],
      "search-algolia/server": [
        "./dist/search-algolia/server.d.ts"
      ],
      "i18n": [
        "./dist/i18n/index.d.ts"
      ]
    }
  },
  "files": [
    "dist/*"
  ],
  "scripts": {
    "build": "tsup",
    "clean": "rimraf dist",
    "dev": "tsup --watch",
    "lint": "eslint .",
    "types:check": "tsc --noEmit"
  },
  "dependencies": {
    "@formatjs/intl-localematcher": "^0.5.4",
    "@shikijs/rehype": "^1.16.2",
    "@shikijs/transformers": "^1.16.2",
    "flexsearch": "0.7.21",
    "github-slugger": "^2.0.0",
    "image-size": "^1.1.1",
    "negotiator": "^0.6.3",
    "npm-to-yarn": "^3.0.0",
    "react-remove-scroll": "^2.5.10",
    "remark": "^15.0.0",
    "remark-gfm": "^4.0.0",
    "remark-mdx": "^3.0.1",
    "scroll-into-view-if-needed": "^3.1.0",
    "shiki": "^1.16.2",
    "swr": "^2.2.5",
    "unist-util-visit": "^5.0.0"
  },
  "devDependencies": {
    "@algolia/client-search": "^4.24.0",
    "@mdx-js/mdx": "^3.0.1",
    "@types/estree-jsx": "^1.0.5",
    "@types/flexsearch": "0.7.6",
    "@types/hast": "^3.0.4",
    "@types/mdast": "^4.0.3",
    "@types/negotiator": "^0.6.3",
    "@types/node": "22.5.4",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "algoliasearch": "^4.24.0",
    "eslint-config-custom": "workspace:*",
    "next": "^14.2.8",
    "tsconfig": "workspace:*",
    "unified": "^11.0.5"
  },
  "peerDependencies": {
    "next": ">= 14.1.0",
    "react": ">= 18",
    "react-dom": ">= 18"
  },
  "publishConfig": {
    "access": "public"
  }
}
